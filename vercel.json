{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "master": true,
      "post-messe": true
    }
  },
  "github": {
    "silent": false,
    "autoAlias": true
  },
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["fra1"],
  "functions": {
    "pages/api/admin/products/[id]/upload-image.js": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
